// Example program demonstrating if-else statements

define main as() -> {
    int x;
    int y;
    int z;

    
    x = 10;
    y = 5;
    if x > y {
        z = 0;
    } else {
        z = 1;
    }

    // Should print out '0'.
    write(z);

    x = 10;
    y = 5;
    if x <= y {
        z = 0;
    } else {
        z = 1;
    }

    // Should print out '1'
    write(z);
}
