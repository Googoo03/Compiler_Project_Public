// A simple program which shows mathematical operations.

define main as() -> {
  int x;
  int y;
  int z;

  x = 10;
  y = 5;

  // This should output '15'
  z = x + y;
  write(z);

  // This should output '5'
  z = x - y;
  write(z);

  // This should output '50'
  z = x * y;
  write(z);

  // This should output '2'
  z = x / y;
  write(z);

  // This should output '0'
  z = x % y;
  write(z);

  // Complex Expression. (5 + 1) * 4
  x = 5;
  y = 4;
  z = (x + 1) * y;
  write(z);
}
